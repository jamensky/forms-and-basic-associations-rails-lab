<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %><br></br>
  <%= f.text_field :title %><br></br>
  <%= f.label :artist[name] %><br></br>
  <%= f.text_field :artist[name]%><br></br>
  <%= f.label :genre %><br></br>
  <%= f.text_field :genre%><br></br>

  <%= f.label :notes%>
  <%= f.text_area :song_notes_1%>
  <%= f.text_area :song_notes_2%>
  <%= f.text_area :song_notes_3%>
  <br/>

  <%= f.submit %>
<% end %>


